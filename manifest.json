{
  "name": "CouponEat",
  "description": "Get coupons with one click",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [],
  "host_permissions": ["https://www.callingtaiwan.com.tw/foodpanda-coupon-code/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/couponeat16.png",
      "32": "images/couponeat32.png",
      "48": "images/couponeat64.png",
      "128": "images/couponeat128.png"
    }
  },
  "icons": {
    "16": "images/couponeat16.png",
    "32": "images/couponeat32.png",
    "48": "images/couponeat64.png",
    "128": "images/couponeat128.png"
  }
}
